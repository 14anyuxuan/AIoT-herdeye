{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport * as echarts from 'echarts';\nimport markerIcon from '@/assets/imgs/icons8-ÂúàÂêë‰∏ä-64.png';\nexport default {\n  name: 'FarmMonitor',\n  data() {\n    return {\n      // Ê†∏ÂøÉÊï∞ÊçÆ\n      weather: {\n        date: `${new Date().getMonth() + 1}Êúà${new Date().getDate()}Êó•`,\n        weekday: ['Âë®Êó•', 'Âë®‰∏Ä', 'Âë®‰∫å', 'Âë®‰∏â', 'Âë®Âõõ', 'Âë®‰∫î', 'Âë®ÂÖ≠'][new Date().getDay()],\n        weather: 'Êô¥',\n        tempMin: 18,\n        tempMax: 30,\n        windDir: '‰∏úÈ£é',\n        windForce: '3Á∫ß'\n      },\n      currDate: new Date().toLocaleDateString(),\n      temp: 22,\n      hum: 50,\n      tempHistory: [{\n        time: '00:00',\n        value: 22\n      }, {\n        time: '00:05',\n        value: 22.5\n      }],\n      humHistory: [{\n        time: '00:00',\n        value: 50\n      }, {\n        time: '00:05',\n        value: 49.5\n      }],\n      tempRange: {\n        min: 18,\n        max: 26\n      },\n      humRange: {\n        min: 40,\n        max: 60\n      },\n      // WebSocketÈÖçÁΩÆ\n      mainWs: null,\n      backendUrl: 'ws://8.152.103.136:9090/ws/data',\n      // ËÆæÂ§áÊï∞ÊçÆ\n      water: {\n        status: 0,\n        value: 0\n      },\n      weight: 50,\n      devices: [{\n        name: 'Ê∞¥Ê≥µ',\n        status: 0\n      }, {\n        name: 'ÁîµÊú∫',\n        status: 0\n      }, {\n        name: 'ÁÅØÂÖâ',\n        status: 0\n      }, {\n        name: 'ÈòÄÈó®',\n        status: 0\n      }, {\n        name: 'Âä†ÁÉ≠',\n        status: 0\n      }, {\n        name: 'È£éÊú∫',\n        status: 0\n      }],\n      // ËøûÊé•‰∏éÈÖçÁΩÆÁä∂ÊÄÅ\n      mainConn: false,\n      mapConn: false,\n      lastUpdate: '-',\n      maxHistory: 10,\n      barHeight: 120,\n      colors: ['#43a047', '#8bc34a', '#cddc39'],\n      weightPercent: 0,\n      tempWarn: false,\n      humWarn: false,\n      waterAlarm: false,\n      gradientBg: 'linear-gradient(to top, #43a047, #8bc34a, #cddc39)',\n      // Âú∞ÂõæÁõ∏ÂÖ≥\n      lng1: 1,\n      lat1: 1,\n      lnglat: [1, 1],\n      course: 0,\n      mapStatus: 'Âú∞ÂõæÂä†ËΩΩ‰∏≠...',\n      map: null,\n      marker: null,\n      // ÂõæË°®ÂÆû‰æã\n      tempChart: null,\n      humChart: null\n    };\n  },\n  computed: {},\n  methods: {\n    // ÂõæË°®ÂàùÂßãÂåñ‰∏éÊ∏≤Êüì\n    initCharts() {\n      const tempDom = document.getElementById('tempChart');\n      const humDom = document.getElementById('humChart');\n      if (!tempDom || !humDom) return;\n      this.tempChart = echarts.init(tempDom);\n      this.humChart = echarts.init(humDom);\n      window.addEventListener('resize', () => {\n        this.tempChart?.resize();\n        this.humChart?.resize();\n      });\n      this.renderCharts();\n    },\n    renderCharts() {\n      // Ê∏©Â∫¶ÂõæË°®ÈÖçÁΩÆ\n      this.tempChart.setOption({\n        tooltip: {\n          trigger: 'axis'\n        },\n        grid: {\n          left: '3%',\n          right: '4%',\n          bottom: '3%',\n          containLabel: true\n        },\n        xAxis: {\n          type: 'category',\n          data: this.tempHistory.map(i => i.time),\n          axisLabel: {\n            rotate: 30\n          }\n        },\n        yAxis: {\n          type: 'value',\n          name: 'Ê∏©Â∫¶(¬∞C)',\n          min: Math.min(...this.tempHistory.map(i => i.value), this.tempRange.min) - 2,\n          max: Math.max(...this.tempHistory.map(i => i.value), this.tempRange.max) + 2,\n          markLine: {\n            data: [{\n              yAxis: this.tempRange.min,\n              name: 'ÊúÄ‰ΩéÈòàÂÄº',\n              lineStyle: {\n                color: 'red'\n              }\n            }, {\n              yAxis: this.tempRange.max,\n              name: 'ÊúÄÈ´òÈòàÂÄº',\n              lineStyle: {\n                color: 'red'\n              }\n            }]\n          }\n        },\n        series: [{\n          name: 'Ê∏©Â∫¶',\n          type: 'line',\n          data: this.tempHistory.map(i => i.value),\n          smooth: true,\n          lineStyle: {\n            color: '#e74c3c'\n          },\n          itemStyle: {\n            color: '#e74c3c'\n          },\n          areaStyle: {\n            color: new echarts.graphic.LinearGradient(0, 0, 0, 1, [{\n              offset: 0,\n              color: 'rgba(231,76,60,0.3)'\n            }, {\n              offset: 1,\n              color: 'rgba(231,76,60,0)'\n            }])\n          }\n        }]\n      });\n\n      // ÊπøÂ∫¶ÂõæË°®ÈÖçÁΩÆ\n      this.humChart.setOption({\n        tooltip: {\n          trigger: 'axis'\n        },\n        grid: {\n          left: '3%',\n          right: '4%',\n          bottom: '3%',\n          containLabel: true\n        },\n        xAxis: {\n          type: 'category',\n          data: this.humHistory.map(i => i.time),\n          axisLabel: {\n            rotate: 30\n          }\n        },\n        yAxis: {\n          type: 'value',\n          name: 'ÊπøÂ∫¶(%)',\n          min: Math.min(...this.humHistory.map(i => i.value), this.humRange.min) - 5,\n          max: Math.max(...this.humHistory.map(i => i.value), this.humRange.max) + 5,\n          markLine: {\n            data: [{\n              yAxis: this.humRange.min,\n              name: 'ÊúÄ‰ΩéÈòàÂÄº',\n              lineStyle: {\n                color: 'blue'\n              }\n            }, {\n              yAxis: this.humRange.max,\n              name: 'ÊúÄÈ´òÈòàÂÄº',\n              lineStyle: {\n                color: 'blue'\n              }\n            }]\n          }\n        },\n        series: [{\n          name: 'ÊπøÂ∫¶',\n          type: 'line',\n          data: this.humHistory.map(i => i.value),\n          smooth: true,\n          lineStyle: {\n            color: '#3498db'\n          },\n          itemStyle: {\n            color: '#3498db'\n          },\n          areaStyle: {\n            color: new echarts.graphic.LinearGradient(0, 0, 0, 1, [{\n              offset: 0,\n              color: 'rgba(52,152,219,0.3)'\n            }, {\n              offset: 1,\n              color: 'rgba(52,152,219,0)'\n            }])\n          }\n        }]\n      });\n    },\n    // ÂéÜÂè≤Êï∞ÊçÆÊõ¥Êñ∞\n    updateHistory(ref, val) {\n      const timeStr = new Date().toLocaleTimeString();\n      ref.push({\n        time: timeStr,\n        value: val\n      });\n      if (ref.length > this.maxHistory) ref.shift();\n    },\n    // Âú∞ÂõæÂàùÂßãÂåñ\n    initMap() {\n      const loadMap = () => new Promise((res, rej) => {\n        if (window.AMap) return res(window.AMap);\n        const script = document.createElement('script');\n        script.src = 'https://webapi.amap.com/maps?v=1.4.15&key=‰Ω†ÁöÑÈ´òÂæ∑Key';\n        script.onload = () => res(window.AMap || rej(new Error('Âú∞ÂõæÂä†ËΩΩÂ§±Ë¥•')));\n        script.onerror = () => rej(new Error('Âú∞ÂõæËÑöÊú¨Âä†ËΩΩÂ§±Ë¥•'));\n        document.head.appendChild(script);\n      });\n      loadMap().then(AMap => {\n        this.map = new AMap.Map('map', {\n          resizeEnable: true,\n          zoom: 16,\n          center: [116.397470, 39.908823],\n          viewMode: '2D'\n        });\n        this.marker = new AMap.Marker({\n          position: [116.397470, 39.908823],\n          icon: new AMap.Icon({\n            size: new AMap.Size(32, 32),\n            image: markerIcon,\n            imageSize: new AMap.Size(64, 64),\n            imageOffset: new AMap.Pixel(-16, -16)\n          }),\n          anchor: 'center'\n        });\n        this.map.add(this.marker);\n        this.map.on('complete', () => this.mapStatus = 'Âú∞ÂõæÂ∑≤Â∞±Áª™');\n        this.map.on('error', () => this.mapStatus = 'Âú∞ÂõæÊ∏≤ÊüìÈîôËØØ');\n      }).catch(err => {\n        this.mapStatus = 'Âú∞ÂõæÂä†ËΩΩÂ§±Ë¥•';\n        console.error(err);\n      });\n    },\n    // WebSocketÂàùÂßãÂåñ\n    initWebSocket() {\n      this.mainWs = new WebSocket(this.backendUrl);\n      this.mainWs.onopen = () => {\n        this.mainConn = true;\n        this.mapConn = true;\n      };\n\n      // Ê†∏ÂøÉÊï∞ÊçÆÊèêÂèñ‰∏éÂ±ïÁ§∫ÈÄªËæë\n      this.mainWs.onmessage = event => {\n        try {\n          const data = JSON.parse(event.data);\n          console.log(data);\n          // Â§ÑÁêÜ‰∏ªÊï∞ÊçÆ\n          if (1) {\n            // ÁéØÂ¢ÉÊï∞ÊçÆÊõ¥Êñ∞ - Ê∑ªÂä†Á±ªÂûãÊ£ÄÊü•‰∏éÈªòËÆ§ÂÄº\n\n            this.temp = typeof data.T === 'number' ? data.T : this.temp;\n            this.tempWarn = this.temp < this.tempRange.min || this.temp > this.tempRange.max;\n            this.hum = typeof data.H === 'number' ? data.H : data.H || this.hum;\n            this.humWarn = this.hum < this.humRange.min || this.hum > this.humRange.max;\n\n            // Ê∞¥‰ΩçÁä∂ÊÄÅÊõ¥Êñ∞\n            this.water = {\n              status: parseInt(data.water_level) || 0,\n              value: parseInt(data.water_level) || 0\n            };\n            this.waterAlarm = this.water.status !== 1;\n\n            // È•≤ÊñôÈáçÈáèÊõ¥Êñ∞\n            this.weight = Math.min(Math.max(data.weight || 0, 0), 100);\n            this.weightPercent = Math.min(Math.max(this.weight, 0), 100);\n\n            // ËÆæÂ§áÁä∂ÊÄÅÊõ¥Êñ∞\n            this.devices[0].status = parseInt(data.pump) || 0; // Ê∞¥Ê≥µ\n            this.devices[1].status = parseInt(data.motor) || 0; // ÁîµÊú∫\n            this.devices[2].status = parseInt(data.light) || 0; // ÁÅØÂÖâ\n            this.devices[3].status = parseInt(data.door) || 0; // ÈòÄÈó®\n            this.devices[4].status = parseInt(data.heat) || 0; // Âä†ÁÉ≠\n            this.devices[5].status = parseInt(data.fan) || 0; // È£éÊú∫\n\n            // Êõ¥Êñ∞ÂéÜÂè≤Êï∞ÊçÆ‰∏éÂõæË°®\n            this.updateHistory(this.tempHistory, this.temp);\n            this.updateHistory(this.humHistory, this.hum);\n            this.lastUpdate = new Date().toLocaleString();\n            this.renderCharts();\n          }\n          // Â§ÑÁêÜÂú∞ÂõæÊï∞ÊçÆ\n          else if (1) {\n            if (data.lng && data.lat) {\n              this.lng1 = data.lng;\n              this.lat1 = data.lat;\n              console.log(this.lng1, this.lat1);\n              this.lnglat = [this.lng1, this.lat1];\n              this.marker?.setPosition([data.lng + 0.01, data.lat - 0.001]);\n              this.map?.panTo([data.lng + 0.01, data.lat - 0.001], {\n                duration: 300\n              });\n              console.log(this.lnglat);\n            }\n            if (data.cogt) {\n              this.course = [data.cogt];\n              this.marker?.setRotation((data.cogt % 360 + 360) % 360);\n            }\n          }\n          console.log(this.lnglat);\n        } catch (e) {\n          console.error('Êï∞ÊçÆËß£ÊûêÂ§±Ë¥•:', e);\n        }\n      };\n      this.mainWs.onerror = () => {\n        this.mainConn = false;\n        this.mapConn = false;\n      };\n      this.mainWs.onclose = () => {\n        this.mainConn = false;\n        this.mapConn = false;\n        setTimeout(() => this.initWebSocket(), 3000);\n      };\n    },\n    // Âà∑Êñ∞Êï∞ÊçÆ\n    refresh() {\n      if (this.mainWs?.readyState === WebSocket.OPEN) {\n        this.mainWs.send(JSON.stringify({\n          type: 'REFRESH_DATA'\n        }));\n        this.lastUpdate = new Date().toLocaleString();\n      }\n    }\n  },\n  mounted() {\n    this.$nextTick(() => {\n      this.initCharts();\n      this.initMap();\n      this.initWebSocket();\n    });\n  },\n  beforeDestroy() {\n    // Ê∏ÖÁêÜËµÑÊ∫ê\n    this.mainWs?.close();\n    this.tempChart?.dispose();\n    this.humChart?.dispose();\n    this.map?.destroy();\n  }\n};","map":{"version":3,"names":["echarts","markerIcon","name","data","weather","date","Date","getMonth","getDate","weekday","getDay","tempMin","tempMax","windDir","windForce","currDate","toLocaleDateString","temp","hum","tempHistory","time","value","humHistory","tempRange","min","max","humRange","mainWs","backendUrl","water","status","weight","devices","mainConn","mapConn","lastUpdate","maxHistory","barHeight","colors","weightPercent","tempWarn","humWarn","waterAlarm","gradientBg","lng1","lat1","lnglat","course","mapStatus","map","marker","tempChart","humChart","computed","methods","initCharts","tempDom","document","getElementById","humDom","init","window","addEventListener","resize","renderCharts","setOption","tooltip","trigger","grid","left","right","bottom","containLabel","xAxis","type","i","axisLabel","rotate","yAxis","Math","markLine","lineStyle","color","series","smooth","itemStyle","areaStyle","graphic","LinearGradient","offset","updateHistory","ref","val","timeStr","toLocaleTimeString","push","length","shift","initMap","loadMap","Promise","res","rej","AMap","script","createElement","src","onload","Error","onerror","head","appendChild","then","Map","resizeEnable","zoom","center","viewMode","Marker","position","icon","Icon","size","Size","image","imageSize","imageOffset","Pixel","anchor","add","on","catch","err","console","error","initWebSocket","WebSocket","onopen","onmessage","event","JSON","parse","log","T","H","parseInt","water_level","pump","motor","light","door","heat","fan","toLocaleString","lng","lat","setPosition","panTo","duration","cogt","setRotation","e","onclose","setTimeout","refresh","readyState","OPEN","send","stringify","mounted","$nextTick","beforeDestroy","close","dispose","destroy"],"sources":["src/views/manager/details.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard\">\r\n    <!-- È°∂ÈÉ®‰ø°ÊÅØÊ†è -->\r\n    <header class=\"top-bar\">\r\n      <div class=\"title-section\">\r\n        <h1>ÂÖªÊÆñÂú∫ÁéØÂ¢ÉÁõëÊéßÁ≥ªÁªü</h1>\r\n        <div class=\"weather-info\">\r\n          <span>{{ weather.date }} {{ weather.weekday }}</span>\r\n          <span>{{ weather.weather }}</span>\r\n          <span>{{ weather.tempMin }}¬∞~{{ weather.tempMax }}¬∞</span>\r\n          <span>{{ weather.windDir }} {{ weather.windForce }}</span>\r\n        </div>\r\n      </div>\r\n      <div class=\"status-info\">\r\n        <div class=\"conn-status\">\r\n          <span class=\"dot\" :class=\"{ green: mainConn }\"></span>\r\n          <span>{{ mainConn ? 'ËøûÊé•Ê≠£Â∏∏' : 'ËøûÊé•Êñ≠ÂºÄ' }}</span>\r\n        </div>\r\n        <span>{{ currDate }}</span>\r\n      </div>\r\n    </header>\r\n\r\n    <!-- Ê†∏ÂøÉÂÜÖÂÆπÂå∫ -->\r\n    <main class=\"main-content\">\r\n      <!-- ÁéØÂ¢ÉÁä∂ÊÄÅÊåáÊ†á -->\r\n      <section class=\"status-cards\">\r\n        <h2>ÁéØÂ¢ÉË∑üË∏™</h2>\r\n        <div class=\"cards\">\r\n          <div class=\"card\" :class=\"{ warn: tempWarn }\">\r\n            <div>ÂΩìÂâçÊ∏©Â∫¶</div>\r\n            <div class=\"val\">{{ typeof temp === 'number' ? temp.toFixed(1) : '0.0' }}¬∞C</div>\r\n          </div>\r\n          <div class=\"card\" :class=\"{ warn: humWarn }\">\r\n            <div>ÂΩìÂâçÊπøÂ∫¶</div>\r\n            <div class=\"val\">{{ typeof hum === 'number' ? hum.toFixed(1) : '0.0' }}%</div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n\r\n      <!-- Ë∂ãÂäøÂõæË°® -->\r\n      <section class=\"charts\">\r\n        <h2>ÁéØÂ¢ÉË∂ãÂäø</h2>\r\n        <div class=\"charts-grid\">\r\n          <div class=\"chart-box\">\r\n            <div>Ê∏©Â∫¶ÂéÜÂè≤Êï∞ÊçÆ</div>\r\n            <div id=\"tempChart\" class=\"chart\"></div>\r\n          </div>\r\n          <div class=\"chart-box\">\r\n            <div>ÊπøÂ∫¶ÂéÜÂè≤Êï∞ÊçÆ</div>\r\n            <div id=\"humChart\" class=\"chart\"></div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n\r\n      <!-- ËÆæÂ§á‰∏éÁä∂ÊÄÅ -->\r\n      <section class=\"device-info\">\r\n        <h2>ËÆæÂ§á‰∏éÁä∂ÊÄÅ‰ø°ÊÅØ</h2>\r\n        <div class=\"info-grid\">\r\n          <div class=\"device-grid\">\r\n            <!-- Ê∞¥‰ΩçÁä∂ÊÄÅ -->\r\n            <div class=\"card\" :class=\"{ warn: waterAlarm }\">\r\n              <div>Ê∞¥‰ΩçÁä∂ÊÄÅ</div>\r\n              <span class=\"status\" :class=\"{ on: water.status === 1 }\">\r\n                {{ water.status === 1 ? 'Ê≠£Â∏∏' : 'ÊúâËØØ' }}\r\n              </span>\r\n            </div>\r\n            \r\n            <!-- È•≤ÊñôÈáçÈáè -->\r\n            <div class=\"card feed-card\">\r\n              <div>È•≤ÊñôÈáçÈáèÁä∂ÊÄÅÂÄº</div>\r\n              <div class=\"progress-container\">\r\n                <div class=\"progress-bar\" :style=\"{ height: barHeight + 'px' }\">\r\n                  <div \r\n                    class=\"progress-fill\" \r\n                    :class=\"{ low: weightPercent < 20 }\"\r\n                    :style=\"{ \r\n                      height: weightPercent + '%',\r\n                      background: gradientBg \r\n                    }\"\r\n                  ></div>\r\n                  <div \r\n                    class=\"progress-marker\"\r\n                    :style=\"{ bottom: weightPercent + '%' }\"\r\n                  ></div>\r\n                </div>\r\n                <div class=\"weight-info\">\r\n                  <span>{{ weight.toFixed(1) }}g</span>\r\n                  <span>{{ weightPercent }}%</span>\r\n                  <span v-if=\"weightPercent < 20\" class=\"low-warning\">\r\n                    È•≤Êñô‰∏çË∂≥ÔºåËØ∑Ë°•ÂÖÖ!\r\n                  </span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            \r\n            <!-- Ê∞¥Ê≥µËÆæÂ§á -->\r\n            <div class=\"card device-card\">\r\n              <div class=\"icon\">üíß</div>\r\n              <div>Ê∞¥Ê≥µÁä∂ÊÄÅ</div>\r\n              <span class=\"status\" :class=\"{ on: devices[0].status === 1 }\">\r\n                {{ devices[0].status === 1 ? 'ËøêË°å‰∏≠' : 'Â∑≤ÂÖ≥Èó≠' }}\r\n              </span>\r\n            </div>\r\n            \r\n            <!-- ÁîµÊú∫ËÆæÂ§á -->\r\n            <div class=\"card device-card\">\r\n              <div class=\"icon\">üîä</div>\r\n              <div>ÁîµÊú∫Áä∂ÊÄÅ</div>\r\n              <span class=\"status\" :class=\"{ on: devices[1].status === 1 }\">\r\n                {{ devices[1].status === 1 ? 'ËøêË°å‰∏≠' : 'Â∑≤ÂÖ≥Èó≠' }}\r\n              </span>\r\n            </div>\r\n            \r\n            <!-- ÁÅØÂÖâËÆæÂ§á -->\r\n            <div class=\"card device-card\">\r\n              <div class=\"icon\">üí°</div>\r\n              <div>ÁÅØÂÖâÁä∂ÊÄÅ</div>\r\n              <span class=\"status\" :class=\"{ on: devices[2].status === 1 }\">\r\n                {{ devices[2].status === 1 ? 'ËøêË°å‰∏≠' : 'Â∑≤ÂÖ≥Èó≠' }}\r\n              </span>\r\n            </div>\r\n            \r\n            <!-- ÈòÄÈó®ËÆæÂ§á -->\r\n            <div class=\"card device-card\">\r\n              <div class=\"icon\">üö™</div>\r\n              <div>ÈòÄÈó®Áä∂ÊÄÅ</div>\r\n              <span class=\"status\" :class=\"{ on: devices[3].status === 1 }\">\r\n                {{ devices[3].status === 1 ? 'ËøêË°å‰∏≠' : 'Â∑≤ÂÖ≥Èó≠' }}\r\n              </span>\r\n            </div>\r\n            \r\n            <!-- Âä†ÁÉ≠ËÆæÂ§á -->\r\n            <div class=\"card device-card\">\r\n              <div class=\"icon\">üî•</div>\r\n              <div>Âä†ÁÉ≠Áä∂ÊÄÅ</div>\r\n              <span class=\"status\" :class=\"{ on: devices[4].status === 1 }\">\r\n                {{ devices[4].status === 1 ? 'ËøêË°å‰∏≠' : 'Â∑≤ÂÖ≥Èó≠' }}\r\n              </span>\r\n            </div>\r\n            \r\n            <!-- È£éÊú∫ËÆæÂ§á -->\r\n            <div class=\"card device-card\">\r\n              <div class=\"icon\">üí®</div>\r\n              <div>È£éÊú∫Áä∂ÊÄÅ</div>\r\n              <span class=\"status\" :class=\"{ on: devices[5].status === 1 }\">\r\n                {{ devices[5].status === 1 ? 'ËøêË°å‰∏≠' : 'Â∑≤ÂÖ≥Èó≠' }}\r\n              </span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n\r\n      <!-- Âú∞ÂõæÂå∫Âüü -->\r\n      <section class=\"map-section\">\r\n        <h2>‰ΩçÁΩÆÁõëÊéß</h2>\r\n        <div class=\"map-container\">\r\n          <div id=\"map\" class=\"map-view\"></div>\r\n          <div class=\"map-status\">\r\n            <span>Âú∞ÂõæÂêéÁ´ØËøûÊé•: </span>\r\n            <span :class=\"{ online: mapConn }\">{{ mapConn ? 'Â∑≤ËøûÊé•' : 'Â∑≤Êñ≠ÂºÄ' }}</span>\r\n            <span class=\"map-dot\" :class=\"{ green: mapConn }\"></span>\r\n          </div>\r\n          <div class=\"map-data\">\r\n            <div><span>ÁªèÁ∫¨Â∫¶:</span> <span>{{ lnglat || 'Á≠âÂæÖÊï∞ÊçÆ...' }}</span></div>\r\n            <div><span>Ëà™ÂêëËßí:</span> <span>{{ course || '0¬∞' }}</span></div>\r\n            <div><span>Âú∞ÂõæÁä∂ÊÄÅ:</span> <span>{{ mapStatus }}</span></div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n    </main>\r\n\r\n    <footer class=\"footer\">\r\n      <div>ÊúÄÂêéÊõ¥Êñ∞Êó∂Èó¥: {{ lastUpdate }}</div>\r\n      <button class=\"refresh\" @click=\"refresh\">\r\n        <i class=\"fa fa-refresh\"></i> Âà∑Êñ∞Êï∞ÊçÆ\r\n      </button>\r\n    </footer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as echarts from 'echarts'\r\nimport markerIcon from '@/assets/imgs/icons8-ÂúàÂêë‰∏ä-64.png'\r\n\r\nexport default {\r\n  name: 'FarmMonitor',\r\n  data() {\r\n    return {\r\n      // Ê†∏ÂøÉÊï∞ÊçÆ\r\n      weather: {\r\n        date: `${new Date().getMonth() + 1}Êúà${new Date().getDate()}Êó•`,\r\n        weekday: ['Âë®Êó•', 'Âë®‰∏Ä', 'Âë®‰∫å', 'Âë®‰∏â', 'Âë®Âõõ', 'Âë®‰∫î', 'Âë®ÂÖ≠'][new Date().getDay()],\r\n        weather: 'Êô¥',\r\n        tempMin: 18,\r\n        tempMax: 30,\r\n        windDir: '‰∏úÈ£é',\r\n        windForce: '3Á∫ß'\r\n      },\r\n      currDate: new Date().toLocaleDateString(),\r\n      temp: 22,\r\n      hum: 50,\r\n      tempHistory: [{ time: '00:00', value: 22 }, { time: '00:05', value: 22.5 }],\r\n      humHistory: [{ time: '00:00', value: 50 }, { time: '00:05', value: 49.5 }],\r\n      tempRange: { min: 18, max: 26 },\r\n      humRange: { min: 40, max: 60 },\r\n\r\n      // WebSocketÈÖçÁΩÆ\r\n      mainWs: null,\r\n      backendUrl: 'ws://8.152.103.136:9090/ws/data',\r\n\r\n      // ËÆæÂ§áÊï∞ÊçÆ\r\n      water: { status: 0, value: 0 },\r\n      weight: 50,\r\n      devices: [\r\n        { name: 'Ê∞¥Ê≥µ', status: 0 },\r\n        { name: 'ÁîµÊú∫', status: 0 },\r\n        { name: 'ÁÅØÂÖâ', status: 0 },\r\n        { name: 'ÈòÄÈó®', status: 0 },\r\n        { name: 'Âä†ÁÉ≠', status: 0 },\r\n        { name: 'È£éÊú∫', status: 0 }\r\n      ],\r\n\r\n      // ËøûÊé•‰∏éÈÖçÁΩÆÁä∂ÊÄÅ\r\n      mainConn: false,\r\n      mapConn: false,\r\n      lastUpdate: '-',\r\n      maxHistory: 10,\r\n      barHeight: 120,\r\n      colors: ['#43a047', '#8bc34a', '#cddc39'],\r\n    weightPercent: 0,\r\n    tempWarn: false,\r\n    humWarn: false,\r\n    waterAlarm: false,\r\n    gradientBg: 'linear-gradient(to top, #43a047, #8bc34a, #cddc39)',\r\n\r\n      // Âú∞ÂõæÁõ∏ÂÖ≥\r\n      lng1: 1,\r\n      lat1: 1,\r\n      lnglat: [1, 1],\r\n      course: 0,\r\n      mapStatus: 'Âú∞ÂõæÂä†ËΩΩ‰∏≠...',\r\n      map: null,\r\n      marker: null,\r\n\r\n      // ÂõæË°®ÂÆû‰æã\r\n      tempChart: null,\r\n      humChart: null\r\n    }\r\n  },\r\n  computed: {\r\n  },\r\n  methods: {\r\n    // ÂõæË°®ÂàùÂßãÂåñ‰∏éÊ∏≤Êüì\r\n    initCharts() {\r\n      const tempDom = document.getElementById('tempChart')\r\n      const humDom = document.getElementById('humChart')\r\n      if (!tempDom || !humDom) return\r\n\r\n      this.tempChart = echarts.init(tempDom)\r\n      this.humChart = echarts.init(humDom)\r\n      \r\n      window.addEventListener('resize', () => {\r\n        this.tempChart?.resize()\r\n        this.humChart?.resize()\r\n      })\r\n      this.renderCharts()\r\n    },\r\n    renderCharts() {\r\n      // Ê∏©Â∫¶ÂõæË°®ÈÖçÁΩÆ\r\n      this.tempChart.setOption({\r\n        tooltip: { trigger: 'axis' },\r\n        grid: { left: '3%', right: '4%', bottom: '3%', containLabel: true },\r\n        xAxis: { \r\n          type: 'category', \r\n          data: this.tempHistory.map(i => i.time),\r\n          axisLabel: { rotate: 30 } \r\n        },\r\n        yAxis: {\r\n          type: 'value',\r\n          name: 'Ê∏©Â∫¶(¬∞C)',\r\n          min: Math.min(...this.tempHistory.map(i => i.value), this.tempRange.min) - 2,\r\n          max: Math.max(...this.tempHistory.map(i => i.value), this.tempRange.max) + 2,\r\n          markLine: {\r\n            data: [\r\n              { yAxis: this.tempRange.min, name: 'ÊúÄ‰ΩéÈòàÂÄº', lineStyle: { color: 'red' } },\r\n              { yAxis: this.tempRange.max, name: 'ÊúÄÈ´òÈòàÂÄº', lineStyle: { color: 'red' } }\r\n            ]\r\n          }\r\n        },\r\n        series: [{\r\n          name: 'Ê∏©Â∫¶',\r\n          type: 'line',\r\n          data: this.tempHistory.map(i => i.value),\r\n          smooth: true,\r\n          lineStyle: { color: '#e74c3c' },\r\n          itemStyle: { color: '#e74c3c' },\r\n          areaStyle: { color: new echarts.graphic.LinearGradient(0,0,0,1,[\r\n            { offset: 0, color: 'rgba(231,76,60,0.3)' },\r\n            { offset: 1, color: 'rgba(231,76,60,0)' }\r\n          ])}\r\n        }]\r\n      })\r\n\r\n      // ÊπøÂ∫¶ÂõæË°®ÈÖçÁΩÆ\r\n      this.humChart.setOption({\r\n        tooltip: { trigger: 'axis' },\r\n        grid: { left: '3%', right: '4%', bottom: '3%', containLabel: true },\r\n        xAxis: { type: 'category', data: this.humHistory.map(i => i.time), axisLabel: { rotate: 30 } },\r\n        yAxis: {\r\n          type: 'value',\r\n          name: 'ÊπøÂ∫¶(%)',\r\n          min: Math.min(...this.humHistory.map(i => i.value), this.humRange.min) - 5,\r\n          max: Math.max(...this.humHistory.map(i => i.value), this.humRange.max) + 5,\r\n          markLine: {\r\n            data: [\r\n              { yAxis: this.humRange.min, name: 'ÊúÄ‰ΩéÈòàÂÄº', lineStyle: { color: 'blue' } },\r\n              { yAxis: this.humRange.max, name: 'ÊúÄÈ´òÈòàÂÄº', lineStyle: { color: 'blue' } }\r\n            ]\r\n          }\r\n        },\r\n        series: [{\r\n          name: 'ÊπøÂ∫¶',\r\n          type: 'line',\r\n          data: this.humHistory.map(i => i.value),\r\n          smooth: true,\r\n          lineStyle: { color: '#3498db' },\r\n          itemStyle: { color: '#3498db' },\r\n          areaStyle: { color: new echarts.graphic.LinearGradient(0,0,0,1,[\r\n            { offset: 0, color: 'rgba(52,152,219,0.3)' },\r\n            { offset: 1, color: 'rgba(52,152,219,0)' }\r\n          ])}\r\n        }]\r\n      })\r\n    },\r\n\r\n    // ÂéÜÂè≤Êï∞ÊçÆÊõ¥Êñ∞\r\n    updateHistory(ref, val) {\r\n      const timeStr = new Date().toLocaleTimeString()\r\n      ref.push({ time: timeStr, value: val })\r\n      if (ref.length > this.maxHistory) ref.shift()\r\n    },\r\n\r\n    // Âú∞ÂõæÂàùÂßãÂåñ\r\n    initMap() {\r\n      const loadMap = () => new Promise((res, rej) => {\r\n        if (window.AMap) return res(window.AMap)\r\n        const script = document.createElement('script')\r\n        script.src = 'https://webapi.amap.com/maps?v=1.4.15&key=‰Ω†ÁöÑÈ´òÂæ∑Key'\r\n        script.onload = () => res(window.AMap || rej(new Error('Âú∞ÂõæÂä†ËΩΩÂ§±Ë¥•')))\r\n        script.onerror = () => rej(new Error('Âú∞ÂõæËÑöÊú¨Âä†ËΩΩÂ§±Ë¥•'))\r\n        document.head.appendChild(script)\r\n      })\r\n\r\n      loadMap().then(AMap => {\r\n        this.map = new AMap.Map('map', {\r\n          resizeEnable: true,\r\n          zoom: 16,\r\n          center: [116.397470, 39.908823],\r\n          viewMode: '2D'\r\n        })\r\n  \r\n        this.marker = new AMap.Marker({\r\n          position: [116.397470, 39.908823],\r\n          icon: new AMap.Icon({\r\n            size: new AMap.Size(32, 32),\r\n            image: markerIcon,\r\n            imageSize: new AMap.Size(64, 64),\r\n            imageOffset: new AMap.Pixel(-16, -16)\r\n          }),\r\n          anchor: 'center'\r\n        })\r\n        this.map.add(this.marker)\r\n        \r\n        this.map.on('complete', () => this.mapStatus = 'Âú∞ÂõæÂ∑≤Â∞±Áª™')\r\n        this.map.on('error', () => this.mapStatus = 'Âú∞ÂõæÊ∏≤ÊüìÈîôËØØ')\r\n      }).catch(err => {\r\n        this.mapStatus = 'Âú∞ÂõæÂä†ËΩΩÂ§±Ë¥•'\r\n        console.error(err)\r\n      })\r\n    },\r\n\r\n    // WebSocketÂàùÂßãÂåñ\r\n    initWebSocket() {\r\n\r\n      this.mainWs = new WebSocket(this.backendUrl)\r\n      \r\n      this.mainWs.onopen = () => {\r\n        this.mainConn = true\r\n        this.mapConn = true\r\n      }\r\n      \r\n      // Ê†∏ÂøÉÊï∞ÊçÆÊèêÂèñ‰∏éÂ±ïÁ§∫ÈÄªËæë\r\n      this.mainWs.onmessage = (event) => {\r\n        try {\r\n          const data = JSON.parse(event.data)\r\n          console.log(data)\r\n          // Â§ÑÁêÜ‰∏ªÊï∞ÊçÆ\r\n          if (1) {\r\n            // ÁéØÂ¢ÉÊï∞ÊçÆÊõ¥Êñ∞ - Ê∑ªÂä†Á±ªÂûãÊ£ÄÊü•‰∏éÈªòËÆ§ÂÄº\r\n      \r\n            this.temp = typeof data.T === 'number' ? data.T : this.temp;\r\n            this.tempWarn = this.temp < this.tempRange.min || this.temp > this.tempRange.max\r\n            this.hum = typeof data.H === 'number' ? data.H : (data.H) || this.hum\r\n            this.humWarn = this.hum < this.humRange.min || this.hum > this.humRange.max\r\n      \r\n            // Ê∞¥‰ΩçÁä∂ÊÄÅÊõ¥Êñ∞\r\n            this.water = {\r\n              status: parseInt(data.water_level) || 0,\r\n              value: parseInt(data.water_level) || 0\r\n            }\r\n            this.waterAlarm = this.water.status !== 1\r\n            \r\n            // È•≤ÊñôÈáçÈáèÊõ¥Êñ∞\r\n            this.weight = Math.min(Math.max((data.weight) || 0, 0), 100)\r\n            this.weightPercent = Math.min(Math.max(this.weight, 0), 100)\r\n            \r\n            // ËÆæÂ§áÁä∂ÊÄÅÊõ¥Êñ∞\r\n            this.devices[0].status = parseInt(data.pump) || 0    // Ê∞¥Ê≥µ\r\n            this.devices[1].status = parseInt(data.motor) || 0   // ÁîµÊú∫\r\n            this.devices[2].status = parseInt(data.light) || 0   // ÁÅØÂÖâ\r\n            this.devices[3].status = parseInt(data.door) || 0    // ÈòÄÈó®\r\n            this.devices[4].status = parseInt(data.heat) || 0    // Âä†ÁÉ≠\r\n            this.devices[5].status = parseInt(data.fan) || 0     // È£éÊú∫\r\n\r\n            // Êõ¥Êñ∞ÂéÜÂè≤Êï∞ÊçÆ‰∏éÂõæË°®\r\n            this.updateHistory(this.tempHistory, this.temp)\r\n            this.updateHistory(this.humHistory, this.hum)\r\n            this.lastUpdate = new Date().toLocal